{
  "manifest_version": 2,

  "name": "MAL Anime Add Helper",
  "description": "This extension helps you add anime to your MAL list easier.",
  "version": "1.0",

  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["add.js"]
      }
    ],
  "browser_action": {
    //"default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "http://*.myanimelist.net/*"
  ]
}
